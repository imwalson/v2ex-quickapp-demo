{
  "package": "com.application.demo.v2ex",
  "name": "v2ex-quickapp-demo",
  "versionName": "1.0.0",
  "versionCode": "1",
  "minPlatformVersion": "101",
  "icon": "/Common/logo.png",
  "features": [
    { "name": "system.prompt" },
    { "name": "system.router" },
    { "name": "system.shortcut" },
    { "name": "system.fetch" },
    { "name": "system.webview" },
    { "name": "system.storage" },
    { "name": "system.clipboard" },
    { "name": "system.share" }
  ],
  "permissions": [
    { "origin": "*" }
  ],
  "config": {
    "logLevel": "debug"
  },
  "router": {
    "entry": "TopicList",
    "pages": {
      "TopicList": {
        "component": "index"
      },
      "Webview": {
        "component": "index"
      },
      "TopicDetails": {
        "component": "index"
      }
    }
  },
  "display": {
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "menu": true,
    "pages": {
      "TopicList": {
        "titleBarText": "V2EX",
        "menu": false
      },
      "TopicDetails": {
        "titleBarText": "帖子详情",
        "menu": false
      },
      "Webview": {
        "titleBarText": "帖子详情",
        "menu": false
      }
    }
  }
}